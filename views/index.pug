<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet", href="/index.css")
    title #{title}
  body
    #main
      #wrap
        - datas.forEach( data => {
          .card
            img(src=data.url, alt="")
            h2.title #{data.title}
            p.date #{data.date}
            p.deskripsi #{data.description}
            p.cr Â© #{data.copyright}
        - })